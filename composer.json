{
    "name": "enriquesomolinos/phpweb-cpd",
    "description": "Copy/Paste Detector for PHP web projects.",
    "homepage": "https://github.com/enriquesomolinos/phpweb-cpd",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Enrique Somolinos",
            "email": "enrique.somolinos@gmail.com",
            "role": "lead"
        },
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "author of phpcpd project https://github.com/sebastianbergmann/phpcpd/"
        }
    ],
    "support": {
        "issues": "https://https://github.com/enriquesomolinos/phpweb-cpd/issues"
    },
    "config": {
        "platform": {
            "php": "7.3.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "sebastian/finder-facade": "^2.0",
        "sebastian/version": "^3.0",
        "symfony/console": "^4.0|^5.0",
        "phpunit/php-timer": "^3.0",
        "friendsoftwig/twigcs": "^3.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "bin": [
        "phpweb-cpd"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
